<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_cfzd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="14dp"
            android:text="发出站点码："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@id/et_start_point"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/et_start_point" />

        <EditText
            android:id="@+id/et_start_point"
            android:layout_width="0dp"
            android:layout_height="27dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="15dp"
            android:background="@drawable/shape_rectangular_in_edit_bg"
            android:drawableRight="@mipmap/ic_scan"
            android:paddingStart="8dp"
            android:paddingEnd="6dp"
            android:textColor="#ff999999"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tv_cfzd"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_end_point"
            android:layout_width="0dp"
            android:layout_height="27dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/shape_rectangular_in_edit_bg"
            android:drawableRight="@mipmap/ic_scan"
            android:paddingStart="8dp"
            android:paddingEnd="6dp"
            android:textColor="#ff999999"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="@id/et_start_point"
            app:layout_constraintStart_toStartOf="@id/et_start_point"
            app:layout_constraintTop_toBottomOf="@id/et_start_point" />

        <EditText
            android:id="@+id/et_gdh"
            android:layout_width="0dp"
            android:layout_height="27dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/shape_rectangular_in_edit_bg"
            android:drawableRight="@mipmap/ic_scan"
            android:paddingStart="8dp"
            android:paddingEnd="6dp"
            android:textColor="#ff999999"
            android:textSize="14sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="@id/et_start_point"
            app:layout_constraintStart_toStartOf="@id/et_start_point"
            app:layout_constraintTop_toBottomOf="@id/et_end_point" />

        <TextView
            android:id="@+id/tv_gdh_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="工单号："
            android:textColor="#ff333333"
            android:textSize="15sp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@id/et_gdh"
            app:layout_constraintStart_toStartOf="@id/tv_tmh"
            app:layout_constraintTop_toTopOf="@id/et_gdh" />

        <TextView
            android:id="@+id/tv_mbzd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="14dp"
            android:text="目标站点码："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@id/et_end_point"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/et_end_point" />

        <EditText
            android:id="@+id/et_tmh"
            android:layout_width="0dp"
            android:layout_height="27dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/shape_rectangular_in_edit_bg"
            android:drawableRight="@mipmap/ic_scan"
            android:paddingStart="8dp"
            android:paddingEnd="6dp"
            android:textColor="#ff999999"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="@id/et_end_point"
            app:layout_constraintStart_toStartOf="@id/et_end_point"
            app:layout_constraintTop_toBottomOf="@id/et_gdh" />

        <TextView
            android:id="@+id/tv_tmh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="14dp"
            android:text="条码号："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@id/et_tmh"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/et_tmh" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:text="物品号："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="@+id/tv_tmh"
            app:layout_constraintTop_toBottomOf="@+id/tv_tmh" />

        <TextView
            android:id="@+id/tv_wph"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:textColor="#ff666666"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView19"
            app:layout_constraintStart_toEndOf="@+id/textView19"
            app:layout_constraintTop_toTopOf="@+id/textView19" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="物品名称："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="@+id/textView19"
            app:layout_constraintTop_toBottomOf="@+id/textView19" />

        <TextView
            android:id="@+id/tv_wpmc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#ff666666"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView20"
            app:layout_constraintStart_toStartOf="@+id/tv_wph"
            app:layout_constraintTop_toTopOf="@+id/textView20" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="物品规格："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="@+id/textView20"
            app:layout_constraintTop_toBottomOf="@+id/textView20" />

        <TextView
            android:id="@+id/tv_gg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#ff666666"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView23"
            app:layout_constraintStart_toStartOf="@+id/tv_wph"
            app:layout_constraintTop_toTopOf="@+id/textView23" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="单位: "
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="@+id/textView23"
            app:layout_constraintTop_toBottomOf="@+id/textView23" />

        <TextView
            android:id="@+id/tv_dw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#ff666666"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView24"
            app:layout_constraintStart_toStartOf="@+id/tv_wph"
            app:layout_constraintTop_toTopOf="@+id/textView24" />

        <TextView
            android:id="@+id/tv_zysl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="转移数量："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="@id/tv_tmh"
            app:layout_constraintTop_toBottomOf="@id/tv_dw" />

        <EditText
            android:id="@+id/et_zysl"
            android:layout_width="115dp"
            android:layout_height="27dp"
            android:background="@drawable/shape_rectangular_in_edit_bg"
            android:paddingStart="14dp"
            android:textColor="#ff999999"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@id/tv_zysl"
            app:layout_constraintStart_toStartOf="@id/et_tmh"
            app:layout_constraintTop_toTopOf="@id/tv_zysl" />

        <EditText
            android:id="@+id/et_fzsl"
            android:layout_width="115dp"
            android:layout_height="27dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/shape_rectangular_in_edit_bg"
            android:inputType="numberDecimal"
            android:paddingStart="14dp"
            android:textColor="#ff999999"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/et_zysl"
            app:layout_constraintTop_toBottomOf="@+id/et_fzdw" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="辅助数量："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@id/et_fzsl"
            app:layout_constraintStart_toStartOf="@+id/textView24"
            app:layout_constraintTop_toTopOf="@+id/et_fzsl" />

        <TextView
            android:id="@+id/et_fzdw"
            android:layout_width="115dp"
            android:layout_height="27dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/shape_rectangular_in_edit_bg"
            android:paddingStart="14dp"
            android:textColor="#ff999999"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/et_fzsl"
            app:layout_constraintTop_toBottomOf="@+id/et_zysl" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="辅助单位："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@id/et_fzdw"
            app:layout_constraintStart_toStartOf="@+id/textView24"
            app:layout_constraintTop_toTopOf="@+id/et_fzdw" />

        <View
            android:id="@+id/line"
            android:layout_width="0dp"
            android:layout_height="0.5dp"
            android:layout_marginTop="16dp"
            android:alpha="0.1"
            android:background="#000000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/et_fzsl" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="操作员："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="@+id/tv_zysl"
            app:layout_constraintTop_toBottomOf="@+id/line" />

        <TextView
            android:id="@+id/tv_user"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:textColor="#ff666666"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView27"
            app:layout_constraintStart_toEndOf="@+id/textView27"
            app:layout_constraintTop_toTopOf="@+id/textView27" />

        <TextView
            android:id="@+id/tv_datetext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:layout_marginBottom="16dp"
            android:text="日期："
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView27"
            app:layout_constraintTop_toBottomOf="@+id/textView27" />

        <TextView
            android:id="@+id/tv_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#ff666666"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_datetext"
            app:layout_constraintStart_toStartOf="@+id/tv_user"
            app:layout_constraintTop_toTopOf="@+id/tv_datetext" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>